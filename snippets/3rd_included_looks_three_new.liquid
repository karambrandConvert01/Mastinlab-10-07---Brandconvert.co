{%- if section.settings.enable == false and request.design_mode == false -%}
{%-else-%}
<div class="dp-section-wrapper n3rd_sections" id="dp__{{section.id}}">
  <div class="page-width">
    <div class="inner-page-width">
      <h2 class="toggle_title {%unless section.settings.default_open %} tactive {%endunless%} toggle_button" style="background-color:{{section.settings.hbackground_color}};color:{{section.settings.htext_color}};">
        {%include "plus_svg"%}
        {{section.settings.headline}}</h2>
      <div class="toggle_content " {% if request.design_mode %}style="max-height: 100% !important;background-color:{{section.settings.background_color}};color:{{section.settings.text_color}};"{%else%}style="background-color:{{section.settings.background_color}};color:{{section.settings.text_color}};"{%endif%}>
        <div class="toggle_inner grid grid--uniform" >
        <div class="grid__item medium-up--one-third">
          <div class="inner_container">
          {%if section.settings.first_image%}
          <div class="right-content-wrapper custom_video_iframe">
		  {%- assign img_url = section.settings.first_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          <img id="foverlay_image_video{{section.id}}" class="lazyload"
               data-src="{{ img_url }}"
               data-widths="[360, 540, 720, 900, 1080, 1600]"
               data-aspectratio="{{ section.settings.image.aspect_ratio }}"
               data-sizes="auto"
               alt="{{ section.settings.first_image.alt }}">
            
          {% if section.settings.fvideo_url == blank %}{% else %}
          	{% if section.settings.fvideo_url.type == "youtube" %}
            	<iframe id="fiframe_video{{section.id}}" data-src="//www.youtube.com/embed/{{ section.settings.fvideo_url.id }}?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" sandbox="allow-scripts allow-same-origin allow-presentation"></iframe>
          	{% endif %}
          	{% if section.settings.fvideo_url.type == "vimeo" %}
            	<iframe id="fiframe_video{{section.id}}" data-src="//player.vimeo.com/video/{{ section.settings.fvideo_url.id }}?color={{ settings.color_button | remove: "#" }}&byline=0&portrait=0&badge=0" sandbox="allow-scripts allow-same-origin allow-presentation" width="850" height="480" frameborder="0" allowfullscreen="allowfullscreen" mozallowfullscreen="mozallowfullscreen" msallowfullscreen="msallowfullscreen" oallowfullscreen="oallowfullscreen" webkitallowfullscreen="webkitallowfullscreen"></iframe>
          	{% endif %}
          {% endif %}
          </div>
          {%endif%} 
            
          <div class="content_wrapper">
            <h3>{{section.settings.ftitle}}</h3>
            <p>{{section.settings.fcontent}}</p>
          </div>
          </div>
        </div>
        <div class="grid__item medium-up--one-third">
          <div class="inner_container">
          {%if section.settings.second_image%}
            <div class="right-content-wrapper custom_video_iframe">
		  {%- assign img_url = section.settings.second_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          <img class="lazyload"
               data-src="{{ img_url }}"
               data-widths="[360, 540, 720, 900, 1080, 1600]"
               data-aspectratio="{{ section.settings.second_image.aspect_ratio }}"
               data-sizes="auto"
               alt="{{ section.settings.second_image.alt }}">
            {% if section.settings.svideo_url == blank %}{% else %}
          	{% if section.settings.svideo_url.type == "youtube" %}
            	<iframe id="siframe_video{{section.id}}" data-src="//www.youtube.com/embed/{{ section.settings.svideo_url.id }}?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" sandbox="allow-scripts allow-same-origin allow-presentation"></iframe>
          	{% endif %}
          	{% if section.settings.svideo_url.type == "vimeo" %}
            	<iframe id="siframe_video{{section.id}}" data-src="//player.vimeo.com/video/{{ section.settings.svideo_url.id }}?color={{ settings.color_button | remove: "#" }}&byline=0&portrait=0&badge=0" sandbox="allow-scripts allow-same-origin allow-presentation" width="850" height="480" frameborder="0" allowfullscreen="allowfullscreen" mozallowfullscreen="mozallowfullscreen" msallowfullscreen="msallowfullscreen" oallowfullscreen="oallowfullscreen" webkitallowfullscreen="webkitallowfullscreen"></iframe>
          	{% endif %}
          {% endif %}
            </div>

          {%endif%}
          <div class="content_wrapper">
            <h3>{{section.settings.stitle}}</h3>
            <p>{{section.settings.scontent}}</p>
          </div>
          </div>
        </div>

        <div class="grid__item medium-up--one-third">
          <div class="inner_container">
          {%if section.settings.third_image%}
            <div class="right-content-wrapper custom_video_iframe">
		  {%- assign img_url = section.settings.third_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          <img class="lazyload"
               data-src="{{ img_url }}"
               data-widths="[360, 540, 720, 900, 1080, 1600]"
               data-aspectratio="{{ section.settings.third_image.aspect_ratio }}"
               data-sizes="auto"
               alt="{{ section.settings.third_image.alt }}">
              
              
              {% if section.settings.tvideo_url == blank %}{% else %}
          	{% if section.settings.svideo_url.type == "youtube" %}
            	<iframe id="tiframe_video{{section.id}}" data-src="//www.youtube.com/embed/{{ section.settings.tvideo_url.id }}?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" sandbox="allow-scripts allow-same-origin allow-presentation"></iframe>
          	{% endif %}
          	{% if section.settings.tvideo_url.type == "vimeo" %}
            	<iframe id="tiframe_video{{section.id}}" data-src="//player.vimeo.com/video/{{ section.settings.tvideo_url.id }}?color={{ settings.color_button | remove: "#" }}&byline=0&portrait=0&badge=0" sandbox="allow-scripts allow-same-origin allow-presentation" width="850" height="480" frameborder="0" allowfullscreen="allowfullscreen" mozallowfullscreen="mozallowfullscreen" msallowfullscreen="msallowfullscreen" oallowfullscreen="oallowfullscreen" webkitallowfullscreen="webkitallowfullscreen"></iframe>
          	{% endif %}
          {% endif %} 
            </div>
            
            
            
          {%endif%}
          <div class="content_wrapper">
            <h3>{{section.settings.ttitle}}</h3>
            <p>{{section.settings.tcontent}}</p>
          </div>
          </div>
        </div>
          
      </div>
      </div>
    </div>
  </div>
</div>
{% style %}
#dp__{{section.id}} .inner-page-width{
	padding:{{section.settings.padding}}
}
@media only screen and (max-width: 768px) {
  #dp__{{section.id}} .inner-page-width{
  	padding:{{section.settings.mobile_padding}};
  }
}
{% endstyle %}
{%endif%}
{% if section.settings.fvideo_url != blank %}
<script>
  ( function( d ) {
   'use strict';
//deproadd
const Player = document.getElementById('fiframe_video{{section.id}}'); 
const PlayBtn = document.getElementById('foverlay_image_video{{section.id}}');
const video_url = Player.getAttribute('data-src');
const playVideo = PlayBtn.addEventListener( 'click' , (ev) => {
      PlayBtn.classList.add( 'hided' );
      Player.src = video_url+"&autoplay=1";
      ev.preventDefault();
});

 }( document ));
</script>
{%endif%}
